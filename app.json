{
  "expo": {
    "name": "NOFARI",
    "slug": "nofari",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "nofari",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#020925"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nofari.nofari7",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#020925"
      },
      "package": "com.nofari.nofari"
    },
    "web": {
      "favicon": "./assets/images/app-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "81210230-952f-45fa-9bcd-55f2551e6729"
      }
    },
    "owner": "nofari",
    "plugins": [
      "expo-audio"
    ]
  }
}
